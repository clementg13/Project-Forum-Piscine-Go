    {{ $categories := .TableCategory }}

    {{ range $category := $categories }}
        <div class="all_categories">
            <div class="categorie">
                <h2>{{ $category.Title }} <span class="category_arrow right_arrow categorie_chevron_active"></span></h2>
                <div id="tech" class="contents" data-value="0">
                    {{ range $subcategory := $category.SubCategory }}

                        <div class="cartouche-content">
                            {{if $subcategory.IsAuthorized}}
                                <i class="fas fa-check" id="valid"></i>
                            {{end}}
                            <div class="bg-cartouche" onclick="location.href='/presentationpost?id={{$subcategory.Id}}'"></div>
                            <div class="cartouche-text">
                                {{$subcategory.Title}}
                                <br/>
                                <span class="numberOfPost">{{.NumberOfPost}} posts</span>
                                <span class="categoryLogo"><i class="{{ .Icon}}"></i></span>
                            </div>
                        </div>
                    {{end}}
                </div>
            </div>
        </div>
